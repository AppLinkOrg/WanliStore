page {
    background-color: #F3F6F7;
}

.order_title {

    font-size: 16px;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #666666;
    line-height: 20px;
    margin: 15px;
}
.input_box{
    background: #FFFFFF;
}
.input_main{
    width: 100%;
    height: 100%;
    border-bottom: 1px solid #E1E3E6;
}
.main_name{
    margin: 15px;
    font-size: 14px;
font-family: PingFangSC-Medium, PingFang SC;
font-weight: bold;
color: #333333;
}
.btns {
    width: 678rpx;
    height: 92rpx;
    /* background: #44B549; */
    box-shadow: 0rpx 4rpx 16rpx 0rpx rgba(68,181,73,0.5);
    border-radius: 48rpx;
    line-height: 92rpx;
    text-align: center;
    color: #FFFFFF;
}
.fixbottom {
    width: 100vw;
    padding: 20rpx 0;
    background: #FFFFFF;
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 10;
}