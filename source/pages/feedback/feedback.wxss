/* pages/feedback/feedback.wxss */
.w {
    width: 690rpx;
}

.textbox {
    width: 690rpx;
    height: 400rpx;
    background: #F6F6F6;
    border-radius: 10rpx;
    box-sizing: border-box;
    padding: 30rpx;
}
.pore {
    position: relative;
}
.uploadimg {
    width: 200rpx;
    height: 200rpx;
    margin-right: 20rpx;
    margin-bottom: 20rpx;
}

.fixbottom {
    width: 100vw;
    padding: 20rpx 0;
    background: #FFFFFF;
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 10;
}
.btns {
    width: 678rpx;
    height: 92rpx;
    /* background: #44B549; */
    box-shadow: 0rpx 4rpx 16rpx 0rpx rgba(68,181,73,0.5);
    border-radius: 48rpx;
    line-height: 92rpx;
    text-align: center;
    color: #FFFFFF;
}
.shanchu {
    width: 70rpx;
    height: 70rpx;
    position: absolute;
    right: 0;
    top: 0;
    z-index: 5;
}