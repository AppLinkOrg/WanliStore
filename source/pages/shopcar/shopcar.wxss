  border-radius: 5px;
}

.btn_s {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 110%;
  font-size: 20px;
}

.btn_1 {
  border-radius: 10px 0 0 10px;
  border-right: 1px solid #dcdcdc;
  color: #Cccc;
}

.btn_2 {
  border-left: 1px solid #dcdcdc;
}

.btn_3 {
  border-radius: 10px 0 0 10px;
  border-right: 1px solid #dcdcdc;
}

.btn_input {
  width: 120%;
  height: 27px;
  display: flex;
  align-items: center;
  text-align: center;
}

