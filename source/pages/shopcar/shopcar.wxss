/* 多选框样式 */
checkbox .wx-checkbox-input {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border: none;
  background: #37C674;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  text-align: center;
  font-size: 32rpx;
  color: #FFF;
  background: transparent;
  /* transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1); */
}

.shop_car_img {
  width: 30%;
  margin-left: 10px;
  display: flex;
  align-items: center;
  border-radius: 10px;
}

.shop_car_imgs {
  width: 100%;
  height: 110px
}

.shopacr_bottom {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 10%;
  background-color: #fafafa;
  z-index: 100;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
